<div id="album_<%= album.id %>" >
   <div><b><%= album.title %></div></b>
      <div><%= link_to image_tag(get_photo_url(album)) , user_album_path(current_user, album) %>  
	<%= link_to 'Edit',  edit_user_album_path(current_user, album) %>
	<%= link_to 'Destroy', user_album_path(current_user, album) , method: :delete,:remote => true,:class => 'delete_photo', data: { confirm: 'Are you sure?' } %>
	<%= link_to 'shared', new_share_album_path(album)%>
      </div>
   </br>
</div>
